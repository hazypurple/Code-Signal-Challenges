    "def checkPalindrome(inputString):\n",
    "    input1 = (inputString)\n",
    "    output = input1[::-1]\n",
    "    \n",
    "    if input1 == output:\n",
    "        result = True\n",
    "    else:\n",
    "        result = False\n",
    "    \n",
    "    return result\n",
    "\n"
   ]
