    "def centuryFromYear(year):\n",
    "    \n",
    "    yr = year/100\n",
    "    \n",
    "    if (yr < 1):\n",
    "        result = 1\n",
    "    elif (yr%(int(yr)) == 0):\n",
    "        result = int(yr)\n",
    "    else:\n",
    "        result = int(yr) + 1\n",
    "        \n",
    "    return result"
   ]
